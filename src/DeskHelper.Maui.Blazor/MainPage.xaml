<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:b="clr-namespace:Microsoft.AspNetCore.Components.WebView.Maui;assembly=Microsoft.AspNetCore.Components.WebView.Maui"
             xmlns:local="clr-namespace:DeskHelper.Maui.Blazor"
             xmlns:blazorpages="clr-namespace:DeskHelper.Blazor;assembly=DeskHelper.Blazor"
             x:Class="DeskHelper.Maui.Blazor.MainPage"
             BackgroundColor="{DynamicResource PageBackgroundColor}"
             Title="DeskHelper">

    <b:BlazorWebView HostPage="wwwroot/_content/DeskHelper.Blazor/index.html">
        <b:BlazorWebView.RootComponents>
            <b:RootComponent Selector="#app" ComponentType="{x:Type blazorpages:RouterPage}" />
        </b:BlazorWebView.RootComponents>
    </b:BlazorWebView>

</ContentPage>